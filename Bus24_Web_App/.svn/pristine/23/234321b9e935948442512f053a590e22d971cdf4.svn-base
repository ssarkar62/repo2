.busRowCard {
	box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.2);
	border-radius: 5px;
	/* 	height: 130px; */
	padding: 20px;
	font-size: 16px;
	min-width: 1000px;
	border: solid #d3d7cf 1px;
	position: relative;
}

.seatLayoutCard {
	background: #FFF;
	border: 5px solid red;
	border-radius: 5px;
	width: 300px;
	padding: 30px;
}

.seatTable  {
	width: 200px;
	height: 100px;
	background-color: #FFF;
	border-radius: 5px;
	border-collapse: separate;
	border-spacing: 2px 2px;}

table.seatTable td, table.seatTable th {
	padding:2px;
	text-align: center;
	width: 10px;
	height: 10px;
}

